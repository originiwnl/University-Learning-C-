#include <iostream>
 
int main(){
    double a, b, c;
 
    a = 1;
    b = 2;
    c = a + b;
 
    std::cout << c << std::endl;
    // this line will print 3
 
    a = 2;
 
    std::cout << c << std::endl;
    // TODO: before testing the program
    // write in a comment below
    // what you expect to be printed
    // As c wasn't updated it will output 3 most likely.
 
    c = a + b;
 
    std::cout << c << std::endl;
    // TODO: before testing the program
    // write in a comment below
    // what you expect to be printed
    // now as c is updated the updated output will be 4.
}